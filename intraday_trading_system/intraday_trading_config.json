{
  "api_provider": "qwen",
  "api_providers": ["qwen","deepseek"],
  "api_config": {
    "qwen": {
      "model": "qwen3-max",
      "base_url": "https://dashscope.aliyuncs.com/compatible-mode/v1",
      "timeout": 300,
      "reasoning_mode": true
    },
    "deepseek": {
      "model": "deepseek-reasoner",
      "base_url": "https://api.deepseek.com/v1",
      "timeout": 300
    }
  },
  "data_config": {
    "history_days": 5,
    "kline_days": 90,
    "cache_enabled": true,
    "api_timeout": 90,
    "max_retries": 2,
    "retry_delay": 3
  },
  "trading_time": {
    "morning_start": "09:30",
    "morning_end": "11:30",
    "afternoon_start": "13:00",
    "afternoon_end": "15:00"
  },
  "risk_control": {
    "max_position_pct": 30,
    "stop_loss_pct": -3,
    "take_profit_pct": 8
  },
  "output_config": {
    "save_to_file": true,
    "output_dir": "intraday_output",
    "show_realtime": true
  },
  "comment": "请在keys.json中配置api_key。api_provider用于单模型模式，api_providers用于多模型对比模式（留空则使用api_provider）"
}

